<Window x:Class="DnD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DnD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55*"/>
            <RowDefinition Height="379*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Height="54" VerticalAlignment="Center">
            <Button Content="заказ" Margin="5,5,5,5" Height="35" Width="100" Click="Button1_Click"/>
            <Button Content="склад" Margin="5,5,5,5" Height="35" Width="100" Click="Button3_Click"/>
            <Button Content="товары" Click="Button2_Click" Height="34" Width="100"/>
        </StackPanel>
        <Frame x:Name="MyFrame" Background="AliceBlue" NavigationUIVisibility="Hidden" Margin="0,55,0,0" Grid.RowSpan="2"/>


    </Grid>
</Window>
